{
  "type": "directory",
  "name": "root",
  "children": [
    {
      "type": "directory",
      "name": "src",
      "children": [
        {
          "type": "file",
          "name": "index.css",
          "content": "@tailwind base;\n@tailwind components;\n@tailwind utilities;"
        },
        {
          "type": "file",
          "name": "main.jsx",
          "content": "import React from 'react'\nimport ReactDOM from 'react-dom/client'\nimport App from './App'\nimport './index.css'\nimport { BrowserRouter } from 'react-router-dom'\n\nReactDOM.createRoot(document.getElementById('root')).render(\n  <BrowserRouter>\n    <App />\n  </BrowserRouter>\n)"
        },
        {
          "type": "file",
          "name": "App.jsx",
          "content": "import React from 'react'\nimport { Routes, Route } from 'react-router-dom'\nimport Header from './components/Header'\nimport Footer from './components/Footer'\nimport Home from './pages/Home'\nimport Products from './pages/Products'\nimport Cart from './pages/Cart'\nimport Checkout from './pages/Checkout'\n\nconst App = () => {\n  return (\n    <div className=\"min-h-screen flex flex-col\">\n      <Header />\n      <main className=\"flex-grow\">\n        <Routes>\n          <Route path=\"/\" element={<Home />} />\n          <Route path=\"/products/:category\" element={<Products />} />\n          <Route path=\"/cart\" element={<Cart />} />\n          <Route path=\"/checkout\" element={<Checkout />} />\n        </Routes>\n      </main>\n      <Footer />\n    </div>\n  )\n}\n\nexport default App"
        },
        {
          "type": "directory",
          "name": "components",
          "children": [
            {
              "type": "file",
              "name": "Header.jsx",
              "content": "import React from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Header = () => {\n  return (\n    <header className=\"bg-gray-900 text-white py-4\">\n      <div className=\"container mx-auto px-4 flex items-center justify-between\">\n        <Link to=\"/\" className=\"text-2xl font-bold\">CameraShop</Link>\n        <nav className=\"hidden md:flex space-x-6\">\n          <Link to=\"/products/dslr\" className=\"hover:text-gray-300\">DSLR</Link>\n          <Link to=\"/products/mirrorless\" className=\"hover:text-gray-300\">Mirrorless</Link>\n          <Link to=\"/products/accessories\" className=\"hover:text-gray-300\">Accessories</Link>\n        </nav>\n        <div className=\"flex items-center space-x-4\">\n          <input type=\"search\" placeholder=\"Search products...\" className=\"px-4 py-1 rounded text-black\" />\n          <Link to=\"/cart\" className=\"hover:text-gray-300\">Cart (0)</Link>\n        </div>\n      </div>\n    </header>\n  )\n}\n\nexport default Header"
            },
            {
              "type": "file",
              "name": "Footer.jsx",
              "content": "import React from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Footer = () => {\n  return (\n    <footer className=\"bg-gray-900 text-white py-8\">\n      <div className=\"container mx-auto px-4 grid grid-cols-1 md:grid-cols-4 gap-8\">\n        <div>\n          <h3 className=\"text-xl font-bold mb-4\">CameraShop</h3>\n          <p>Your trusted source for professional cameras and accessories.</p>\n        </div>\n        <div>\n          <h4 className=\"font-bold mb-4\">Quick Links</h4>\n          <ul className=\"space-y-2\">\n            <li><Link to=\"/shipping\" className=\"hover:text-gray-300\">Shipping Info</Link></li>\n            <li><Link to=\"/returns\" className=\"hover:text-gray-300\">Return Policy</Link></li>\n            <li><Link to=\"/contact\" className=\"hover:text-gray-300\">Contact Us</Link></li>\n          </ul>\n        </div>\n        <div>\n          <h4 className=\"font-bold mb-4\">Connect With Us</h4>\n          <div className=\"flex space-x-4\">\n            <a href=\"#\" className=\"hover:text-gray-300\">Facebook</a>\n            <a href=\"#\" className=\"hover:text-gray-300\">Twitter</a>\n            <a href=\"#\" className=\"hover:text-gray-300\">Instagram</a>\n          </div>\n        </div>\n      </div>\n    </footer>\n  )\n}\n\nexport default Footer"
            }
          ]
        },
        {
          "type": "directory",
          "name": "pages",
          "children": [
            {
              "type": "file",
              "name": "Home.jsx",
              "content": "import React from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Home = () => {\n  return (\n    <div className=\"container mx-auto px-4 py-8\">\n      <section className=\"mb-12\">\n        <div className=\"bg-gray-100 rounded-lg p-8 flex flex-col md:flex-row items-center\">\n          <div className=\"md:w-1/2\">\n            <h1 className=\"text-4xl font-bold mb-4\">Professional Cameras for Every Photographer</h1>\n            <p className=\"mb-6\">Discover our wide range of cameras and accessories.</p>\n            <Link to=\"/products/all\" className=\"bg-blue-600 text-white px-6 py-2 rounded hover:bg-blue-700\">\n              Shop Now\n            </Link>\n          </div>\n          <div className=\"md:w-1/2\">\n            <img src=\"https://images.unsplash.com/photo-1516035069371-29a1b244cc32\" alt=\"Camera\" className=\"rounded-lg\" />\n          </div>\n        </div>\n      </section>\n\n      <section className=\"mb-12\">\n        <h2 className=\"text-2xl font-bold mb-6\">Featured Products</h2>\n        <div className=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n          {[1, 2, 3].map((item) => (\n            <div key={item} className=\"bg-white p-4 rounded-lg shadow\">\n              <img src={`https://source.unsplash.com/random/400x300?camera=${item}`} alt=\"Product\" className=\"w-full h-48 object-cover rounded mb-4\" />\n              <h3 className=\"font-bold\">Professional Camera</h3>\n              <p className=\"text-gray-600\">$999.99</p>\n            </div>\n          ))}\n        </div>\n      </section>\n    </div>\n  )\n}\n\nexport default Home"
            }
          ]
        }
      ]
    },
    {
      "type": "file",
      "name": "index.html",
      "content": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <title>CameraShop - Professional Cameras and Accessories</title>\n  </head>\n  <body>\n    <div id=\"root\"></div>\n    <script type=\"module\" src=\"/src/main.jsx\"></script>\n  </body>\n</html>"
    }
  ]
}