{
  "type": "directory",
  "name": "coding-institute",
  "children": [
    {
      "type": "directory",
      "name": "src",
      "children": [
        {
          "type": "file",
          "name": "main.jsx",
          "content": "import React from 'react'\nimport ReactDOM from 'react-dom/client'\nimport App from './App'\nimport './index.css'\nimport { BrowserRouter } from 'react-router-dom'\n\nReactDOM.createRoot(document.getElementById('root')).render(\n  <React.StrictMode>\n    <BrowserRouter>\n      <App />\n    </BrowserRouter>\n  </React.StrictMode>,\n)"
        },
        {
          "type": "file",
          "name": "index.css",
          "content": "@tailwind base;\n@tailwind components;\n@tailwind utilities;"
        },
        {
          "type": "file",
          "name": "App.jsx",
          "content": "import React from 'react'\nimport { Routes, Route } from 'react-router-dom'\nimport Navbar from './components/Navbar'\nimport Footer from './components/Footer'\nimport Home from './pages/Home'\nimport Courses from './pages/Courses'\nimport About from './pages/About'\nimport Testimonials from './pages/Testimonials'\nimport Register from './pages/Register'\nimport Contact from './pages/Contact'\nimport Blog from './pages/Blog'\n\nconst App = () => {\n  return (\n    <div className=\"min-h-screen flex flex-col\">\n      <Navbar />\n      <main className=\"flex-grow\">\n        <Routes>\n          <Route path=\"/\" element={<Home />} />\n          <Route path=\"/courses\" element={<Courses />} />\n          <Route path=\"/about\" element={<About />} />\n          <Route path=\"/testimonials\" element={<Testimonials />} />\n          <Route path=\"/register\" element={<Register />} />\n          <Route path=\"/contact\" element={<Contact />} />\n          <Route path=\"/blog\" element={<Blog />} />\n        </Routes>\n      </main>\n      <Footer />\n    </div>\n  )\n}\n\nexport default App"
        },
        {
          "type": "directory",
          "name": "components",
          "children": [
            {
              "type": "file",
              "name": "Navbar.jsx",
              "content": "import React, { useState } from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Navbar = () => {\n  const [isOpen, setIsOpen] = useState(false)\n\n  return (\n    <nav className=\"bg-blue-600 text-white\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"flex items-center justify-between h-16\">\n          <Link to=\"/\" className=\"font-bold text-xl\">CodeInstitute</Link>\n          <div className=\"hidden md:block\">\n            <div className=\"ml-10 flex items-baseline space-x-4\">\n              <Link to=\"/\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Home</Link>\n              <Link to=\"/courses\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Courses</Link>\n              <Link to=\"/about\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">About</Link>\n              <Link to=\"/testimonials\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Testimonials</Link>\n              <Link to=\"/register\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Register</Link>\n              <Link to=\"/contact\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Contact</Link>\n              <Link to=\"/blog\" className=\"hover:bg-blue-700 px-3 py-2 rounded-md\">Blog</Link>\n            </div>\n          </div>\n          <button\n            onClick={() => setIsOpen(!isOpen)}\n            className=\"md:hidden p-2\"\n          >\n            <svg className=\"h-6 w-6\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n              {isOpen ? (\n                <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth={2} d=\"M6 18L18 6M6 6l12 12\" />\n              ) : (\n                <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth={2} d=\"M4 6h16M4 12h16M4 18h16\" />\n              )}\n            </svg>\n          </button>\n        </div>\n      </div>\n      {isOpen && (\n        <div className=\"md:hidden\">\n          <div className=\"px-2 pt-2 pb-3 space-y-1 sm:px-3\">\n            <Link to=\"/\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Home</Link>\n            <Link to=\"/courses\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Courses</Link>\n            <Link to=\"/about\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">About</Link>\n            <Link to=\"/testimonials\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Testimonials</Link>\n            <Link to=\"/register\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Register</Link>\n            <Link to=\"/contact\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Contact</Link>\n            <Link to=\"/blog\" className=\"block hover:bg-blue-700 px-3 py-2 rounded-md\">Blog</Link>\n          </div>\n        </div>\n      )}\n    </nav>\n  )\n}\n\nexport default Navbar"
            },
            {
              "type": "file",
              "name": "Footer.jsx",
              "content": "import React from 'react'\n\nconst Footer = () => {\n  return (\n    <footer className=\"bg-gray-800 text-white py-8\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 md:grid-cols-3 gap-8\">\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">CodeInstitute</h3>\n            <p className=\"text-gray-400\">Empowering the next generation of developers</p>\n          </div>\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">Contact</h3>\n            <p className=\"text-gray-400\">Email: info@codeinstitute.com</p>\n            <p className=\"text-gray-400\">Phone: (123) 456-7890</p>\n          </div>\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">Follow Us</h3>\n            <div className=\"flex space-x-4\">\n              <a href=\"#\" className=\"text-gray-400 hover:text-white\">Facebook</a>\n              <a href=\"#\" className=\"text-gray-400 hover:text-white\">Twitter</a>\n              <a href=\"#\" className=\"text-gray-400 hover:text-white\">LinkedIn</a>\n            </div>\n          </div>\n        </div>\n        <div className=\"mt-8 border-t border-gray-700 pt-8 text-center text-gray-400\">\n          <p>Â© 2024 CodeInstitute. All rights reserved.</p>\n        </div>\n      </div>\n    </footer>\n  )\n}\n\nexport default Footer"
            }
          ]
        },
        {
          "type": "directory",
          "name": "pages",
          "children": [
            {
              "type": "file",
              "name": "Home.jsx",
              "content": "import React from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Home = () => {\n  return (\n    <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <div className=\"text-center\">\n        <h1 className=\"text-4xl font-bold mb-4\">Learn Coding with Industry Experts</h1>\n        <p className=\"text-xl text-gray-600 mb-8\">Start your journey to becoming a professional developer today</p>\n        <Link to=\"/courses\" className=\"bg-blue-600 text-white px-6 py-3 rounded-lg hover:bg-blue-700\">Explore Courses</Link>\n      </div>\n      \n      <div className=\"mt-16 grid grid-cols-1 md:grid-cols-3 gap-8\">\n        <div className=\"bg-white p-6 rounded-lg shadow-md\">\n          <h3 className=\"text-xl font-bold mb-4\">Expert Instructors</h3>\n          <p className=\"text-gray-600\">Learn from industry professionals with years of experience</p>\n        </div>\n        <div className=\"bg-white p-6 rounded-lg shadow-md\">\n          <h3 className=\"text-xl font-bold mb-4\">Practical Projects</h3>\n          <p className=\"text-gray-600\">Build real-world applications as you learn</p>\n        </div>\n        <div className=\"bg-white p-6 rounded-lg shadow-md\">\n          <h3 className=\"text-xl font-bold mb-4\">Job Ready Skills</h3>\n          <p className=\"text-gray-600\">Get the skills employers are looking for</p>\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default Home"
            },
            {
              "type": "file",
              "name": "Courses.jsx",
              "content": "import React from 'react'\n\nconst courses = [\n  {\n    id: 1,\n    title: 'Web Development Fundamentals',\n    description: 'Learn HTML, CSS, and JavaScript basics',\n    duration: '8 weeks',\n    price: '$499',\n    level: 'Beginner'\n  },\n  {\n    id: 2,\n    title: 'Advanced React Development',\n    description: 'Master React and modern frontend development',\n    duration: '10 weeks',\n    price: '$699',\n    level: 'Intermediate'\n  },\n  {\n    id: 3,\n    title: 'Python for Data Science',\n    description: 'Learn Python programming for data analysis',\n    duration: '12 weeks',\n    price: '$799',\n    level: 'Intermediate'\n  }\n]\n\nconst Courses = () => {\n  return (\n    <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-12\">\n      <h2 className=\"text-3xl font-bold mb-8\">Our Courses</h2>\n      <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8\">\n        {courses.map(course => (\n          <div key={course.id} className=\"bg-white p-6 rounded-lg shadow-md\">\n            <h3 className=\"text-xl font-bold mb-2\">{course.title}</h3>\n            <p className=\"text-gray-600 mb-4\">{course.description}</p>\n            <div className=\"text-sm text-gray-500\">\n              <p>Duration: {course.duration}</p>\n              <p>Price: {course.price}</p>\n              <p>Level: {course.level}</p>\n            </div>\n            <button className=\"mt-4 bg-blue-600 text-white px-4 py-2 rounded hover:bg-blue-700\">Learn More</button>\n          </div>\n        ))}\n      </div>\n    </div>\n  )\n}\n\nexport default Courses"
            }
          ]
        }
      ]
    }
  ]
}