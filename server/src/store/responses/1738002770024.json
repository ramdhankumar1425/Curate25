{
  "type": "directory",
  "name": "root",
  "children": [
    {
      "type": "directory",
      "name": "src",
      "children": [
        {
          "type": "file",
          "name": "main.jsx",
          "content": "import React from 'react'\nimport ReactDOM from 'react-dom/client'\nimport App from './App'\nimport './index.css'\nimport { BrowserRouter } from 'react-router-dom'\n\nReactDOM.createRoot(document.getElementById('root')).render(\n  <React.StrictMode>\n    <BrowserRouter>\n      <App />\n    </BrowserRouter>\n  </React.StrictMode>\n)"
        },
        {
          "type": "file",
          "name": "index.css",
          "content": "@tailwind base;\n@tailwind components;\n@tailwind utilities;"
        },
        {
          "type": "file",
          "name": "App.jsx",
          "content": "import React from 'react'\nimport { Routes, Route } from 'react-router-dom'\nimport Navbar from './components/Navbar'\nimport Footer from './components/Footer'\nimport Home from './pages/Home'\nimport About from './pages/About'\nimport Courses from './pages/Courses'\nimport Faculty from './pages/Faculty'\nimport Resources from './pages/Resources'\nimport Contact from './pages/Contact'\n\nconst App = () => {\n  return (\n    <div className=\"min-h-screen flex flex-col\">\n      <Navbar />\n      <main className=\"flex-grow\">\n        <Routes>\n          <Route path=\"/\" element={<Home />} />\n          <Route path=\"/about\" element={<About />} />\n          <Route path=\"/courses\" element={<Courses />} />\n          <Route path=\"/faculty\" element={<Faculty />} />\n          <Route path=\"/resources\" element={<Resources />} />\n          <Route path=\"/contact\" element={<Contact />} />\n        </Routes>\n      </main>\n      <Footer />\n    </div>\n  )\n}\n\nexport default App"
        },
        {
          "type": "directory",
          "name": "components",
          "children": [
            {
              "type": "file",
              "name": "Navbar.jsx",
              "content": "import React, { useState } from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Navbar = () => {\n  const [isOpen, setIsOpen] = useState(false)\n\n  return (\n    <nav className=\"bg-blue-600 text-white\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"flex items-center justify-between h-16\">\n          <Link to=\"/\" className=\"font-bold text-xl\">CodeTech Institute</Link>\n          \n          <div className=\"hidden md:block\">\n            <div className=\"flex items-center space-x-4\">\n              <Link to=\"/\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">Home</Link>\n              <Link to=\"/about\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">About</Link>\n              <Link to=\"/courses\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">Courses</Link>\n              <Link to=\"/faculty\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">Faculty</Link>\n              <Link to=\"/resources\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">Resources</Link>\n              <Link to=\"/contact\" className=\"hover:bg-blue-700 px-3 py-2 rounded\">Contact</Link>\n            </div>\n          </div>\n\n          <button\n            className=\"md:hidden\"\n            onClick={() => setIsOpen(!isOpen)}\n          >\n            <svg className=\"h-6 w-6\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\">\n              <path strokeLinecap=\"round\" strokeLinejoin=\"round\" strokeWidth={2} d={isOpen ? \"M6 18L18 6M6 6l12 12\" : \"M4 6h16M4 12h16M4 18h16\"} />\n            </svg>\n          </button>\n        </div>\n      </div>\n\n      {isOpen && (\n        <div className=\"md:hidden\">\n          <div className=\"px-2 pt-2 pb-3 space-y-1 sm:px-3\">\n            <Link to=\"/\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">Home</Link>\n            <Link to=\"/about\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">About</Link>\n            <Link to=\"/courses\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">Courses</Link>\n            <Link to=\"/faculty\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">Faculty</Link>\n            <Link to=\"/resources\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">Resources</Link>\n            <Link to=\"/contact\" className=\"block hover:bg-blue-700 px-3 py-2 rounded\">Contact</Link>\n          </div>\n        </div>\n      )}\n    </nav>\n  )\n}\n\nexport default Navbar"
            },
            {
              "type": "file",
              "name": "Footer.jsx",
              "content": "import React from 'react'\n\nconst Footer = () => {\n  return (\n    <footer className=\"bg-gray-800 text-white py-8\">\n      <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n        <div className=\"grid grid-cols-1 md:grid-cols-3 gap-8\">\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">CodeTech Institute</h3>\n            <p className=\"text-gray-300\">Empowering future developers with quality education</p>\n          </div>\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">Contact Info</h3>\n            <p className=\"text-gray-300\">Email: info@codetech.edu</p>\n            <p className=\"text-gray-300\">Phone: (555) 123-4567</p>\n          </div>\n          <div>\n            <h3 className=\"text-xl font-bold mb-4\">Follow Us</h3>\n            <div className=\"flex space-x-4\">\n              <a href=\"#\" className=\"text-gray-300 hover:text-white\">Twitter</a>\n              <a href=\"#\" className=\"text-gray-300 hover:text-white\">LinkedIn</a>\n              <a href=\"#\" className=\"text-gray-300 hover:text-white\">Facebook</a>\n            </div>\n          </div>\n        </div>\n        <div className=\"mt-8 border-t border-gray-700 pt-8 text-center\">\n          <p className=\"text-gray-300\">&copy; 2024 CodeTech Institute. All rights reserved.</p>\n        </div>\n      </div>\n    </footer>\n  )\n}\n\nexport default Footer"
            }
          ]
        },
        {
          "type": "directory",
          "name": "pages",
          "children": [
            {
              "type": "file",
              "name": "Home.jsx",
              "content": "import React from 'react'\nimport { Link } from 'react-router-dom'\n\nconst Home = () => {\n  return (\n    <div>\n      <section className=\"bg-blue-600 text-white py-20\">\n        <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <div className=\"text-center\">\n            <h1 className=\"text-4xl md:text-6xl font-bold mb-6\">Learn Coding with Excellence</h1>\n            <p className=\"text-xl mb-8\">Master software development, DSA, and more with industry experts</p>\n            <Link to=\"/courses\" className=\"bg-white text-blue-600 px-8 py-3 rounded-full font-bold hover:bg-gray-100\">\n              Explore Courses\n            </Link>\n          </div>\n        </div>\n      </section>\n\n      <section className=\"py-16\">\n        <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <h2 className=\"text-3xl font-bold text-center mb-12\">Our Key Offerings</h2>\n          <div className=\"grid grid-cols-1 md:grid-cols-3 gap-8\">\n            <div className=\"p-6 bg-white rounded-lg shadow-lg\">\n              <h3 className=\"text-xl font-bold mb-4\">Software Development</h3>\n              <p>Comprehensive courses in modern programming languages and frameworks.</p>\n            </div>\n            <div className=\"p-6 bg-white rounded-lg shadow-lg\">\n              <h3 className=\"text-xl font-bold mb-4\">Data Structures & Algorithms</h3>\n              <p>Master problem-solving skills with our intensive DSA program.</p>\n            </div>\n            <div className=\"p-6 bg-white rounded-lg shadow-lg\">\n              <h3 className=\"text-xl font-bold mb-4\">Industry Projects</h3>\n              <p>Work on real-world projects with experienced mentors.</p>\n            </div>\n          </div>\n        </div>\n      </section>\n    </div>\n  )\n}\n\nexport default Home"
            },
            {
              "type": "file",
              "name": "About.jsx",
              "content": "import React from 'react'\n\nconst About = () => {\n  return (\n    <div className=\"max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-16\">\n      <h1 className=\"text-4xl font-bold mb-8\">About Us</h1>\n      <div className=\"grid grid-cols-1 md:grid-cols-2 gap-12\">\n        <div>\n          <p className=\"text-lg mb-6\">\n            CodeTech Institute is a leading coding education center dedicated to nurturing the next generation of software developers and technology leaders.\n          </p>\n          <p className=\"text-lg mb-6\">\n            Our mission is to provide high-quality, industry-relevant education that prepares students for successful careers in technology.\n          </p>\n        </div>\n        <div>\n          <img \n            src=\"https://images.unsplash.com/photo-1516321318423-f06f85e504b3\"\n            alt=\"About Us\"\n            className=\"rounded-lg shadow-lg\"\n          />\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default About"
            }
          ]
        }
      ]
    }
  ]
}