              "name": "Login.jsx",
              "content": "import React from 'react';\n\nconst Login = () => {\n    return (\n        <div data-curate-edit-id=\"1dc1c1fb-483d-4177-9fb1-7913a7946ffa\" className=\"hero min-h-screen bg-base-200\">\n            <div data-curate-edit-id=\"447b50ac-71c2-41ab-8789-be2f6ed480e0\" className=\"hero-content flex-col lg:flex-row-reverse\">\n                <div data-curate-edit-id=\"3bea9126-5e98-4ec5-8a1f-14c7887e5ff8\" className=\"card shrink-0 w-full max-w-sm shadow-2xl bg-base-100\">\n                    <form data-curate-edit-id=\"4d371afe-45b5-4320-a584-24fbe874f011\" className=\"card-body\">\n                        <div data-curate-edit-id=\"77d8c150-be7b-47aa-8005-88205a18c0c1\" className=\"form-control\">\n                            <label data-curate-edit-id=\"e4385574-d323-4b30-a8ee-aed3e4851228\" className=\"label\">\n                                <span data-curate-edit-id=\"0e829db5-378b-4951-a7dd-d3bfd558f4ad\" className=\"label-text\">Email</span>\n                            </label>\n                            <input data-curate-edit-id=\"f7da91c3-b3f7-4149-a883-d72cf79c87d0\" type=\"email\" placeholder=\"email\" className=\"input input-bordered\" required />\n                        </div>\n                        <div data-curate-edit-id=\"4ff668ba-eb5a-448a-a6bd-e3fdcf97c3fc\" className=\"form-control\">\n                            <label data-curate-edit-id=\"1f91e477-98af-4c06-82f8-10e55dff33ed\" className=\"label\">\n                                <span data-curate-edit-id=\"65e602d9-8ddd-4e88-b413-737789957b65\" className=\"label-text\">Password</span>\n                            </label>\n                            <input data-curate-edit-id=\"63197a00-481f-47a2-9e59-e6fd087cf65c\" type=\"password\" placeholder=\"password\" className=\"input input-bordered\" required />\n                        </div>\n                        <div data-curate-edit-id=\"cae388b4-bcf7-4cf9-a15b-d72cb8011bfb\" className=\"form-control mt-6\">\n  